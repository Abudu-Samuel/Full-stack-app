{
  "name": "full-stack-app",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:prod": "npm run build:prod && npm start",
    "start:dev": "concurrently \"npm run build:dev:server\" \"npm run build:dev:client\"",
    "build:dev": "npm run build:dev:client && npm run build:dev:server",
    "build:dev:client": "webpack-dev-server --mode development",
    "build:dev:server": "nodemon src/server/server.js --exec babel-node",
    "build:prod:client": "webpack --mode production",
    "build:prod:server": "babel src/server/ -d dist/",
    "build:prod": "npm run build:prod:client && npm run build:prod:server",
    "heroku-postbuild": "npm run build:prod",
    "test": "nyc --reporter=html --reporter=text mocha --timeout 500000 --require @babel/register --require @babel/polyfill src/server/test/*test.js --watch --watch-extensions js",
    "test:frontend": "jest --config jest.config.json -u",
    "test:client": "npm run test:frontend -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abudu-Samuel/Full-stack-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Abudu-Samuel/Full-stack-app/issues"
  },
  "homepage": "https://github.com/Abudu-Samuel/Full-stack-app#readme",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "compression": "^1.7.3",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "jest": "^23.6.0",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "mongoose": "^5.4.1",
    "morgan": "^1.9.1",
    "moxios": "^0.4.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
